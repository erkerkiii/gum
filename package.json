  "publishConfig": {
    "ignore": [
      "Resources/",
      "Gum.Composer/bin",
      "Gum.Composer/obj",
      "Gum.Core/bin",
      "Gum.Core/obj",
      "Gum.Core/.idea",
      "Gum.Sandbox",
      "Gum.Pooling/bin",
      "Gum.Pooling/obj",
      "Gum.Signal/obj",
      "Gum.Signal/bin",
      "Gum.WebRequest/obj",
      "Gum.WebRequest/bin",
      "Tests/obj",
      "Tests/bin",
      ".idea",
